{
  "Defaults": {
    "GeneratedCodeFolder": "generated",
    "FunctionMaxAllowedRunningMinutes": 15,
    "ConsoleCommands": {
      "Az": {
        "ListFunctionApps": "functionapp list -g [settings.AzureSettings.ResourceGroup] --query \"[].{Name:name}\"",
        "ListFunctions": "functionapp function list -g [settings.AzureSettings.ResourceGroup] -n [AppName] --query \"[].{Name:name}\"",
        "DeleteFunctionApp": "functionapp delete -g [settings.AzureSettings.ResourceGroup] -n [AppName]",
        "StartFunctionApp": "functionapp start -g [settings.AzureSettings.ResourceGroup] -n [AppName]",
        "StopFunctionApp": "functionapp stop -g [settings.AzureSettings.ResourceGroup] -n [AppName]"
      },
      "Func": {
        "LocalSimulateFunction": "start -g [settings.AzureSettings.ResourceGroup]",
        "ShowLogStream": "azure functionapp logstream [AppName]"
      },
      "DotNet": {
        "CleanProject": "clean --configuration Release /property:GenerateFullPaths=true",
        "BuildProject": "build --configuration Release /property:GenerateFullPaths=true",
        "PublishProject": "publish --configuration Release /property:GenerateFullPaths=true"
      },
      "Lambda": {
        "ShowLogStream": "azure functionapp logstream [AppName]" 
      }
    }
  },
  "CtlRepository": {
    "SystemStateTableName": "system_state",
    "ObjectStateTableName": "object_state"}
}