{
  "Defaults": {
    "GeneratedCodeFolder": "generated",
    
    // commands
    
    "DotNetCleanProject": "clean --configuration Release /property:GenerateFullPaths=true",
    "DotNetBuildProject": "build --configuration Release /property:GenerateFullPaths=true",
    "DotNetPublishProject": "publish --configuration Release /property:GenerateFullPaths=true",
    
    "AzListFunctionAppsCmd": "functionapp list -g [settings.AzureSettings.ResourceGroup] --query \"[].{Name:name}\"",
    "AzListFunctionsCmd": "functionapp function list -g [settings.AzureSettings.ResourceGroup] -n [AppName] --query \"[].{Name:name}\"",
    "AzDeleteFunctionAppCmd": "functionapp delete -g [settings.AzureSettings.ResourceGroup] -n [AppName]",
    "AzStartFunctionAppCmd": "functionapp start -g [settings.AzureSettings.ResourceGroup] -n [AppName]",
    "AzStopFunctionAppCmd": "functionapp stop -g [settings.AzureSettings.ResourceGroup] -n [AppName]"
  }
}