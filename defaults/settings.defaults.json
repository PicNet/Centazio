{
  "Defaults": {
    "GeneratedCodeFolder": "generated",
    "ConsoleCommands": {
      "Az": {
        "ListFunctionAppsCmd": "functionapp list -g [settings.AzureSettings.ResourceGroup] --query \"[].{Name:name}\"",
        "ListFunctionsCmd": "functionapp function list -g [settings.AzureSettings.ResourceGroup] -n [AppName] --query \"[].{Name:name}\"",
        "DeleteFunctionAppCmd": "functionapp delete -g [settings.AzureSettings.ResourceGroup] -n [AppName]",
        "StartFunctionAppCmd": "functionapp start -g [settings.AzureSettings.ResourceGroup] -n [AppName]",
        "StopFunctionAppCmd": "functionapp stop -g [settings.AzureSettings.ResourceGroup] -n [AppName]"
      },
      "Func": {
        "LocalSimulateFunctionCmd": "start -g [settings.AzureSettings.ResourceGroup]"
      },
      "DotNet": {
        "CleanProject": "clean --configuration Release /property:GenerateFullPaths=true",
        "BuildProject": "build --configuration Release /property:GenerateFullPaths=true",
        "PublishProject": "publish --configuration Release /property:GenerateFullPaths=true"
      }
    }
  },
  "CtlRepository": {
    "SystemStateTableName": "system_state",
    "ObjectStateTableName": "object_state"
  }
}